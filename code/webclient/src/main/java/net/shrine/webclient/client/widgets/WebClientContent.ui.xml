<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui" 
			 xmlns:shrine="urn:import:net.shrine.webclient.client.widgets">
			 
	<g:HTMLPanel styleName="mainContent">
        <!--Search Box -->
		<g:SimplePanel styleName="searchBox" ui:field="searchBoxHolder"/>
        <!-- Data Dictionary -->
        <shrine:DataDictionaryRow ui:field="dataDictionaryRow"/>  
		<!-- Query groups -->
		<shrine:QueryPanel ui:field="queryPanel" />
		<!-- Query Summary -->
		<shrine:SummaryPanel ui:field="summaryPanel" />
        <!-- Query Results -->
		<shrine:ResultsPanel ui:field="resultsPanel" />
	</g:HTMLPanel>
</ui:UiBinder> 