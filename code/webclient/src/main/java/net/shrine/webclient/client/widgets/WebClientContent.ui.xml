<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui" 
			 xmlns:shrine="urn:import:net.shrine.webclient.client.widgets">
			 
	<g:HTMLPanel styleName="mainContent">
        <g:HTMLPanel styleName="searchBox">
		  <!--Search Box -->
		  <g:SimplePanel ui:field="searchBoxHolder"/>
		  <!-- Data Dictionary -->
		  <shrine:DataDictionaryRow ui:field="dataDictionaryRow"/>
        </g:HTMLPanel>  
		<!-- Query groups -->
		<shrine:QueryPanel ui:field="queryPanel" />
		<!-- Breakdown Row -->
		<shrine:SummaryPanel ui:field="summaryPanel" />
	</g:HTMLPanel>
</ui:UiBinder> 