<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui" 
			 xmlns:shrine="urn:import:net.shrine.webclient.client.widgets">
			 
	<g:HTMLPanel styleName="mainContent">
		<!--Search Box -->
		<g:SimplePanel ui:field="searchBoxHolder" addStyleNames="searchBox"/>
		<!-- Data Dictionary -->
		<shrine:DataDictionaryRow ui:field="dataDictionaryRow"/>
		<!-- Query groups -->
		<shrine:QueryColumn ui:field="queryColumn" />
		<!-- Breakdown Row -->
		<shrine:AllResultsRow ui:field="allResultColumn" />
	</g:HTMLPanel>
</ui:UiBinder> 