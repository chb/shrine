<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui" 
			 xmlns:shrine="urn:import:net.shrine.webclient.client.widgets">
			 
	<g:HTMLPanel>
		<!--Search Box -->
		<shrine:SearchArea ui:field="searchArea" />
		<!-- Breakdown (RHS) -->
		<shrine:AllResultColumn ui:field="allResultColumn" />
		<!-- Query groups -->
		<shrine:QueryColumn ui:field="queryColumn" />
		<div id="queryMenu" class="queryTermMenu">
			<a href="#">View in Browser</a>
			<a href="#">Set/View individual filters</a>
		</div>
		<div class="footer"></div>
	</g:HTMLPanel>
</ui:UiBinder> 