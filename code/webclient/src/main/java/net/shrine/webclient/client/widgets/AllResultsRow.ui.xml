<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="breakDownOuter">
		<div class="breakDown">
			<div class="breakDownTop">
				<div class="breakDownTopLeft">
					<div class="breakDownTopRight">
						Approved Query Topic:
						<br />
						<select name="datasteward" class="dataDropdown">
							<option value="1">Diabetes and medications</option>
                      		<option value="2">Request New Query Topic</option>
						</select>
						<g:Button text="Run Query" addStyleNames="largeButton" ui:field="runQueryButton"/>
					</div>
					<g:SimplePanel ui:field="querySummaryHolder"/>
				</div>
			</div>
			<g:HTMLPanel styleName="breakDownUpper" ui:field="resultsWrapper">
				<div class="resultInstitutionsTitle">Institution Results</div>
				<g:FlowPanel styleName="resultInstitutions" ui:field="resultsPanel" />
			</g:HTMLPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 