<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="breakDownOuter">
		<div class="breakDown">
			<div class="breakDownTop">
				<div class="breakDownTopLeft">
					<div class="breakDownTopRight">
						Approved Query Topic:
						<a href="#">Request New Query Topic</a>
						<br />
						<select name="datastewart" class="dataDropdown">
							<option value="1">Option 1</option>
							<option value="2">Option 2</option>
						</select>
						<g:Button text="Run Query" addStyleNames="largeButton" ui:field="runQueryButton"/>
					</div>
					<g:SimplePanel ui:field="querySummaryHolder"/>
				</div>
			</div>
			<div class="breakDownUpper">
				<div class="resultInstitutionsTitle"> Institution Results</div>
				<g:FlowPanel styleName="resultInstitutions" ui:field="resultsPanel" />
				<div class="queryResultsTotals" style="display: none;"> 25,870 </div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 