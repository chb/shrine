<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:HTMLPanel styleName="breakDown">
        <div class="resultInstitutionsTitle">
            <div class="resultLink">
                <a href="#">
                    <img width="30" height="30" align="absmiddle" border="0" alt="Get Patient Set" src="images/download.png" />
                </a>
                <a href="#">Request Patient Set</a>
            </div>
            Institution Results
        </div>

        <div class="breakDownInstitution">
            <div class="resultDescription">Click each institution below to view the breakdown for that institution.</div>
            <g:FlowPanel ui:field="resultsInstitutionsDelegate" styleName="resultInstitutions"/>
        </div>
        <g:SimplePanel ui:field="resultDetails" styleName="breakDownDemographics"/>

        <div style="clear: both;"></div>
    </g:HTMLPanel>
</ui:UiBinder> 