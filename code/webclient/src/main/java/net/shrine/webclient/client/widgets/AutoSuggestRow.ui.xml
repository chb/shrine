<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="autosuggest">
		<span class="terms">
			<g:Label ui:field="simpleName"/>
			<g:Label ui:field="synonym" styleName="synonym"/>
		</span>
        <span class="icon"><g:Anchor ui:field="browseLink"><img ui:field="iconImage"/></g:Anchor></span>
	</g:HTMLPanel>
</ui:UiBinder> 